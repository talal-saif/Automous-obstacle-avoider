{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Gazebo Plugin",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/gazebo",
            "args": [
                "${workspaceFolder}/sim/worlds/aoa.world"
            ],
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "Build (root)",
            "setupCommands": [
                {
                    "description": "Enable pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug Raspberry Pi Executable",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/pi/aoa_pi",
            "args": [],
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "Build (root)",
            "setupCommands": [
                {
                    "description": "Enable pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}